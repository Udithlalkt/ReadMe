public class PolicyBatchRenewalRq {

    @XmlElement(name = "ClientInfo", namespace = "http://www.avivacanada.com/services/policyintegrationservice/policybatchrenewal/v1")
    private ClientInfo clientInfo;

    @XmlElement(name = "PrimaryPolicySystemId", namespace = "http://www.avivacanada.com/services/policyintegrationservice/policybatchrenewal/v1")
    private String primaryPolicySystemId;

    @XmlElement(name = "RenewalDetail", namespace = "http://www.avivacanada.com/services/policyintegrationservice/policybatchrenewal/v1")
    private List<RenewalDetail> renewalDetails;

    // Getters and Setters
}



