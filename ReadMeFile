

<xsd:group name="SERVICERQPULL_CHOICE">
    <xsd:choice>
        <xsd:sequence>
            <xsd:group ref="INSURANCESVCSPIRQMSGS" maxOccurs="unbounded"/>
            <xsd:group ref="SURETYSVCSPIRQMSGS" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:appinfo>
                        <jaxb:property name="suretySvcRequestMessagesSequence"/>
                    </jaxb:appinfo>
                </xsd:annotation>
            </xsd:group>
        </xsd:sequence>
        <xsd:group ref="SURETYSVCSPIRQMSGS" maxOccurs="unbounded">
            <xsd:annotation>
                <xsd:appinfo>
                    <jaxb:property name="suretySvcRequestMessagesChoice"/>
                </jaxb:appinfo>
            </xsd:annotation>
        </xsd:group>
    </xsd:choice>
</xsd:group>

<jxb:bindings version="2.1" xmlns:jxb="http://java.sun.com/xml/ns/jaxb" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <jxb:bindings schemaLocation="MergedSchema.xsd" node="/xs:schema">
        <jxb:bindings node="//xs:group[@ref='SURETYSVCSPIRQMSGS']">
            <jxb:property name="suretySvcRequestMessages"/>
        </jxb:bindings>
    </jxb:bindings>
</jxb:bindings>


<plugin>
    <groupId>org.codehaus.mojo</groupId>
    <artifactId>jaxb2-maven-plugin</artifactId>
    <version>3.1.0</version>
    <executions>
        <execution>
            <id>xjc</id>
            <goals>
                <goal>xjc</goal>
            </goals>
        </execution>
    </executions>
    <configuration>
        <sources>
            <source>src/main/resources/xsd/MergedSchema.xsd</source>
        </sources>
        <outputDirectory>src/main/java</outputDirectory>
        <clearOutputDir>false</clearOutputDir>
        <bindingDirectory>src/main/resources</bindingDirectory>
        <bindingFiles>
            <bindingFile>bindings.xjb</bindingFile>
        </bindingFiles>
    </configuration>
</plugin>
